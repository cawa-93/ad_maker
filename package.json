{
  "name": "ad_maker",
  "version": "0.2.1",
  "description": "Быстрое заполнение Коммандера",
  "main": "app/main.js",
  "scripts": {
    "start": "electron ./app",
    "build": "build -lw --x64 --ia32",
    "build:dev": "build -l --x64",
    "build:win":"build -w --ia32 --prerelease"
  },
  "author": "Alex Kozack <kozackunisoft@gmail.com>",
  "repository": "cawa-93/ad_maker",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron-builder": "^10.9.2",
    "electron-prebuilt": "^1.4.13"
  },
  "dependencies": {},
  "build": {
    "appId": "com.electron.admaker",
    "linux": {
      "target": "deb",
      "category": "Utility"
    },
    "nsis": {
      "license": "./LICENSE",
      "language": "1058"
    },
    "win": {
      "iconUrl": "https://github.com/cawa-93/ad_maker/blob/master/build/icon.ico?raw=true"      
    },
    "publish": [{
      "provider": "github",
      "repo": "cawa-93/ad_maker"
    }],
    "squirrelWindows": {
      "iconUrl": "https://github.com/cawa-93/ad_maker/blob/master/build/icon.ico?raw=true",
      "remoteReleases": true
    }
  },
  "directories": {
    "app": "./app"
  }
}
