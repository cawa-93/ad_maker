{
  "name": "command-editor",
  "productName": "Command.Editor",
  "version": "4.0.2",
  "description": "Быстрый редактор кампаний из Яндекс.Директ Коммандера",
  "repository": "cawa-93/command-editor",
  "author": "Alex Kozack <kozackunisoft@gmail.com>",
  "license": "GPL-3.0",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "docker": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron  --env-file ./.env electronuserland/electron-builder:wine",
    "compile": "electron-webpack",
    "release": "yarn compile && electron-builder -w",
    "dist": "yarn compile && electron-builder --dir -c.compression=store",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "build": {
    "productName": "Command.Editor",
    "appId": "org.command-editor",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^2.8.0",
    "fast-csv": "^2.4.0",
    "iconv-lite": "^0.4.18",
    "jschardet": "^1.5.0",
    "lodash.clonedeep": "^4.5.0",
    "material-design-icons": "^3.0.1",
    "nedb": "^1.8.0",
    "transliteration": "^1.6.2",
    "vue": "^2.4.2",
    "vue-electron": "^1.0.6",
    "vuetify": "^0.14.7",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.1.0",
    "electron": "^1.7.5",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.3.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-webpack": "^0.10.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.7",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^13.0.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
